<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.deluxe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.ChatActivity"></activity>
        <activity android:name=".View.ChatUserActivity">

        </activity>
        <activity android:name=".View.WithdrawActivity" />
        <activity android:name=".View.TransferActivity" />
        <activity android:name=".View.SearchUserActivity" />
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.DepositActivity" />
        <activity android:name=".View.SignUpActivity" />
        <activity android:name=".View.SignInActivity" /> <!-- [START firebase_service] -->
        <service
            android:name=".FCM.FirebaseMessageReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>