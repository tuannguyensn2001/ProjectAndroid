<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".View.MainActivity">

	<Button
		android:id="@+id/NapThe"
		style="@style/SubmitButton"
		android:layout_width="320dp"
		android:text="NapThe"
		app:layout_constraintBottom_toTopOf="@+id/transfer"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<Button
		android:id="@+id/transfer"
		style="@style/SubmitButton"
		android:layout_width="320dp"
		android:text="Chuyen tien"
		app:layout_constraintBottom_toTopOf="@+id/ruttien"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/NapThe" />

	<Button
		android:id="@+id/ruttien"
		style="@style/SubmitButton"
		android:layout_width="320dp"
		android:text="Rut tien"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/transfer" />

</androidx.constraintlayout.widget.ConstraintLayout>
